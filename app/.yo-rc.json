{
  "generator-fireloop": {
    "version": "1.0.0-beta.2.7",
    "clients": {
      "fireloop": {
        "path": "./fireloop",
        "type": "server"
      },
      "webapp": {
        "path": "./webapp",
        "type": "ng2web",
        "name": "webapp",
        "sdkPath": "webapp/src/app/shared/sdk",
        "serve": "ng serve -H 0.0.0.0"
      }
    },
    "promptValues": {
      "sdkFeatures": [
        "Enable PubSub + IO + FireLoop functionality"
      ]
    }
  }
}